<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal">
<head>
  <title></title>
	<link type="text/css" href="${request.application_url}/static/css/custom-theme/style.css" rel="stylesheet" />
	<link type="text/css" href="${request.application_url}/static/css/site.css" rel="stylesheet" />
	<script type="text/javascript" src="${request.application_url}/static/js/jquery-1.3.2.min.js"></script>
	<script type="text/javascript" src="${request.application_url}/static/js/jquery-ui-1.7.2.custom.min.js"></script>
</head>
<body>
	<div id="artist-results" class="results">
	  <h2>Artists</h2>
	     <ul>
		<li tal:repeat="artist in artists"><h3 tal:content="unicode(artist)">Artist</h3></li>
	     </ul>
	</div>
	<div id="album-results" class="results">
	  <h2>Albums</h2>
	     <ul>
		<li tal:repeat="album in albums"><h3 tal:content="u'%s - (%s)' % (unicode(album),unicode(album.artist))">Album</h3></li>
	     </ul>
	</div>
	<div id="track-results" class="results">
	  <h2>Tracks</h2>
	     <ul>
		<li tal:repeat="track in tracks"><h3 tal:content="u'%s - (%s)' % (unicode(track),unicode(track.artist))">Tracks</h3></li>
	     </ul>
	</div>
</body>
</html>
