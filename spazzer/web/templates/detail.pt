<ul xmlns:tal="http://xml.zope.org/namespaces/tal"
    id="album-list" class="data-list list">
    <li class="artist-item" tal:repeat="item albums">
      <h3>${unicode(item)}&nbsp;&nbsp;<a class="download-link" href="${'%s/artists/%s/albums/%s/download' % (request.application_url,unicode(artist.name) ,unicode(item.name))}">(download)</a></h3>
	<ol class="album-content">
	  <li tal:repeat="track in item.get_tracks()">
	      <span tal:content="track.title or '(Unknown)'">track</span>&nbsp;&nbsp;<a class="download-link" href="${'%s/tracks/%s/download' % (request.application_url,str(track.id))}">download</a>
	   </li>
	</ol>
    </li>
</ul>
