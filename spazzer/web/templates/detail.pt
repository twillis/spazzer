<ul xmlns:tal="http://xml.zope.org/namespaces/tal"
    class="album-list data-list list">
    <li class="artist-item" tal:repeat="item albums">
      <h3>${unicode(item)}&nbsp;&nbsp;<a class="download-link" href="${'%s/download/?artist=%s&amp;album=%s' % (request.application_url,request.url_quote(unicode(artist.name if artist else '')) ,request.url_quote(unicode(item.name)))}">(download)</a></h3>
	<ol class="album-content">
	  <li tal:repeat="track in item.get_tracks()">
	      <span tal:content="track.title or '(Unknown)'">track</span>&nbsp;&nbsp;<a class="download-link" href="${'%s/download/?track=%s' % (request.application_url,str(track.id))}">download</a>
	   </li>
	</ol>
    </li>
</ul>
